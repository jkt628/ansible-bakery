[connection]
id=jkt-rv
type=wifi
autoconnect-retries=0
autoconnect-priority=-1
llmnr=2
mdns=2

[wifi]
band=bg
cloned-mac-address=preserve
mode=infrastructure
powersave=2
ssid=jkt-rv

[wifi-security]
auth-alg=open
key-mgmt=wpa-psk
psk={{ jkt_rv_psk }}

[ipv4]
method=auto

[ipv6]
ip6-privacy=0
method=auto
